super + p
	dmenu_run

super + w
	firefox

super + l
	slock

super + shift + Return
	st

super + F4
	~/scripts/shutdown

XF86Audio{Lower,Raise}Volume
	;~/scripts/change-volume {down,up}

XF86AudioMute
	;~/scripts/change-volume mute

XF86MonBrightness{Down,Up}
	;~/scripts/change-brightness {down,up}

Print
	xfce4-screenshooter --window --clipboard --save "$HOME/Imagens/screenshots/$(date +"%Y-%m-%d-%s").png"

control + Print
	xfce4-screenshooter --region --clipboard --save "$HOME/Imagens/screenshots/$(date +"%Y-%m-%d-%s").png"
